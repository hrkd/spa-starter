  "scripts": {
    "start": "HTTPS=true env-cmd -f .env.development react-scripts start",
    "build": "rm -fr build && REACT_APP_COMMIT_HASH=$(git rev-parse HEAD) env-cmd -f .env.development react-scripts build",
    "build:stg": "rm -fr build && REACT_APP_COMMIT_HASH=$(git rev-parse HEAD) env-cmd -f .env.staging react-scripts build",
    "build:prod": "rm -fr build && REACT_APP_COMMIT_HASH=$(git rev-parse HEAD) env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
